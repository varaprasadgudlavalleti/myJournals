KPex	$PE_setvalue_d;//for PE - eps 0.59 in setfieldsdict
KPey	$PE_setvalue_d;
KPez	$PE_setvalue_d;
betaPex	0;
betaPey	0;
betaPez	0;


KDIx	$DI_setvalue_d; //for DI - eps 0.59 in setfieldsdict
KDIy	$DI_setvalue_d;
KDIz	$DI_setvalue_d;
betaDIx	0;
betaDIy	0;
betaDIz	0;



KNex	$NE_setvalue_d;//for NE - eps 0.59 in setfieldsdict
KNey	$NE_setvalue_d;
KNez	$NE_setvalue_d;
betaNex	0;
betaNey	0;
betaNez	0;



porosity_PE
{
    type            explicitPorositySource;

    explicitPorositySourceCoeffs
    {
        selectionMode   cellZone;
        cellZone        PE;
        type            DarcyForchheimer;

        d   ($KPex $KPey $KPez);
    	f   ($betaPex $betaPey $betaPez);

    	coordinateSystem
    	{
        	origin  (0 0 0);
        	e1      (1 0 0);
        	e2      (0 1 0);
    	}

    }
}





porosity_DI
{
    type            explicitPorositySource;

    explicitPorositySourceCoeffs
    {
        selectionMode   cellZone;
        cellZone        DI;
        type            DarcyForchheimer;

        d   ($KDIx $KDIy $KDIz);
    	f   ($betaDIx $betaDIy $betaDIz);

    	coordinateSystem
    	{
        	origin  (0 0 0);
        	e1      (1 0 0);
        	e2      (0 1 0);
    	}
    }
}



