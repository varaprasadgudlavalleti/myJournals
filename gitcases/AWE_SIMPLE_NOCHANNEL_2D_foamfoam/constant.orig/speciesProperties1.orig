FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

gas
{

}

electrolyte
{
//**********Ionic Charges (z_*):**********//
//These define the electrical charges for each species. For example, hydroxide (OH⁻) carries a negative unit charge and potassium (K⁺) a positive unit charge.
//The charges (OH⁻: –1, K⁺: +1) are standard and do not depend on concentration.
    z_OH	set_z_OH;		//charge
	z_H2O	set_z_waterH2O;		//charge
	z_H2	set_z_H2;		//charge
	z_O2	set_z_O2;		//charge
	z_K		set_z_K;		//charge



//**********Ion Mobilities (u_*): **********//
//The mobilities (units: m²/(V·s) in SI) determine how fast the ions move in an electric field.
//These values are lower than in dilute solutions, which is expected for a concentrated 30 wt% KOH electrolyte due to higher viscosity.
	u_OH	set_u_OH;	//Mobility
	u_K		set_u_K;	//Mobility



//**********Diffusion-Related Constants **********//
//Activation Energies (Ea): These values are used to adjust the diffusion coefficients with temperature via an Arrhenius relation.

	Ea_H2O		set_Ea_waterH2O;
	Ea_OH		set_Ea_OH;
	Ea_H2		set_Ea_H2;
	Ea_O2		set_Ea_O2;

//Reference Diffusion Coefficients (D_ref): These values represent the baseline diffusion coefficients at specified reference temperatures.
	D_H2O_ref	set_D_waterH2O_ref;
	D_OH_ref	set_D_OH_ref;//5.27e-9;
	D_H2_ref	set_D_H2_ref;
	D_O2_ref	set_D_O2_ref;

//Reference Temperatures (T_ref):Notice that the liquid species (H₂O and OH⁻) are referenced at 353 K (≈80°C), a typical operating temperature for alkaline water electrolysis, whereas the gas species (H₂ and O₂) use 298 K.
	T_ref_H2	set_T_ref_H2;
	T_ref_O2	set_T_ref_O2;
	T_ref_H2O	set_T_ref_waterH2O;
	T_ref_OH	set_T_ref_OH;
//**********Diffusion-Related Constants **********//

}


//Molecular Weights (MW):These are standard molecular weights for the respective species.

MW_H2		set_MW_H2;	//kg/mol
MW_O2		set_MW_O2;	//kg/mol
MW_OH		set_MW_OH;	//kg/mol
MW_H2O		set_MW_waterH2O;	//kg/mol
MW_K		set_MW_K;	//kg/mol


