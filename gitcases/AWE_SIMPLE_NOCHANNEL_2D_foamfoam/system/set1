FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      set1;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

set1{
type sets;

interpolationScheme cellPoint;
setFormat raw;
fields
	(
 		p_rgh C_H2.electrolyte C_H2.gas C_O2.electrolyte C_O2.gas C_OH.electrolyte C_H2O.electrolyte U Ue UPe UNe eta J alpha.gas rhod rho Psi_si_O2 Psi_si_H2
 	);
 	writeControl    timeStep;
	writeInterval   200;

	writeFields     true;
	log             false;
sets
 	(
		YLineEnd
		{
			type uniform;
			axis	y;
			start	(0.09 0 0.015);
			end	(0.09 0.00445 0.015);
			nPoints	60;
		//	triangulate true;
		}
		YLineMid
		{
			type uniform;
			axis	y;
			start	(0.05 0 0.015);
			end	(0.05 0.00445 0.015);
			nPoints	60;
		//	triangulate true;
		}
		YLineEarly
		{
			type uniform;
			axis	y;
			start	(0.01 0 0.015);
			end	(0.01 0.00445 0.015);
			nPoints	60;
		//	triangulate true;
		}

   );
		
}    
  
