//**********************************************************************************************************************//
PE_setvalue_poredensity						110;
PE_setvalue_poresize						2.0000e-04;
PE_setvalue_tortuosity						1.20;
PE_setvalue_specificsurfacearea 			1.4141e+04;
PE_setvalue_viscosity						2.0000e-03;
PE_setvalue_porosity						0.5892;
PE_setvalue_permeability					1.7290e-10;
PE_setvalue_d								1.1568e+07;
PE_setvalue_porous_wire_dia					1.5455e-05;
PE_setvalue_porouscell_spacing				2.3091e-04;
PE_setvalue_esimatedsherwoodnumber			4.9101e-01;




DI_setvalue_poredensity						300;
DI_setvalue_poresize						5.0000e-05;
DI_setvalue_tortuosity						1.50;
DI_setvalue_specificsurfacearea 			3.2869e+04;
DI_setvalue_viscosity						2.0000e-03;
DI_setvalue_porosity						0.2739;
DI_setvalue_permeability					3.9309e-13;
DI_setvalue_d								5.0879e+09;
DI_setvalue_porous_wire_dia					1.7333e-05;
DI_setvalue_porouscell_spacing				8.4667e-05;
DI_setvalue_esimatedsherwoodnumber			1.8261e-01;



NE_setvalue_poredensity						110;
NE_setvalue_poresize						2.0000e-04;
NE_setvalue_tortuosity						1.20;
NE_setvalue_specificsurfacearea 			1.4141e+04;
NE_setvalue_viscosity						2.0000e-03;
NE_setvalue_porosity						0.5892;
NE_setvalue_permeability					1.7290e-10;
NE_setvalue_d								1.1568e+07;
NE_setvalue_porous_wire_dia					1.5455e-05;
NE_setvalue_porouscell_spacing				2.3091e-04;
NE_setvalue_esimatedsherwoodnumber			4.9101e-01;
//**********************************************************************************************************************//



defaultFieldValues
(
    //volScalarFieldValue sigma_s 1000
    //volScalarFieldValue r_p 2e-05
    volScalarFieldValue Ne 0
    volScalarFieldValue Pe 0
    volScalarFieldValue Mem 0
    //volScalarFieldValue InRegion 0
    volScalarFieldValue eps 0.9999
    volScalarFieldValue	tau 1
);





regions
(


//eps ALLOTMENT

//epsallotment for PE region
    boxToCell
    {
    	box (0 0 $z0) ($tp $H $z1);

    	fieldValues
    	(
    		volScalarFieldValue eps $PE_setvalue_porosity
    	);
    }
//epsallotment for PE region

//epsallotment for DI region
    boxToCell
    {
    	box ($tp 0 $z0) ($t1 $H $z1);

    	fieldValues
    	(
    		volScalarFieldValue eps $DI_setvalue_porosity
    	);
    }
//epsallotment for DI region

//epsallotment for NE region
        boxToCell
    {
    	box ($t1 0 $z0) ($t2 $H $z1);

    	fieldValues
    	(
    		volScalarFieldValue eps $NE_setvalue_porosity
    	);
    }
//epsallotment for NE region

//eps ALLOTMENT






//tau ALLOTMENT

//tau allotment for PE region
    boxToCell
    {
    	box (0 0 $z0) ($tp $H $z1);

    	fieldValues
    	(
    		volScalarFieldValue tau $PE_setvalue_tortuosity
    	);
    }
//tau allotment for PE region

//tau allotment for DI region
    boxToCell
    {
    	box ($tp 0 $z0) ($t1 $H $z1);

    	fieldValues
    	(
    		volScalarFieldValue tau $DI_setvalue_tortuosity
    	);
    }
//tau allotment for DI region

//tau allotment for NE region
        boxToCell
    {
    	box ($t1 0 $z0) ($t2 $H $z1);

    	fieldValues
    	(
    		volScalarFieldValue tau $NE_setvalue_tortuosity
    	);
    }
//tau allotment for NE region

//tau ALLOTMENT










//volScalarFieldValue ALLOTMENT


//PE
    boxToCell
    {
    	box (0 0 $z0) ($tp $H $z1);

    	fieldValues
    	(
    		volScalarFieldValue Pe 1
    	);
    }
//PE

//DI
        boxToCell
    {
    	box ($tp 0 $z0) ($t1 $H $z1);

    	fieldValues
    	(
    		volScalarFieldValue Mem 1
    	);
    }
//DI

//NE
    boxToCell
    {
    	box ($t1 0 $z0) ($t2 $H $z1);

    	fieldValues
    	(
    		volScalarFieldValue Ne 1
    	);
    }
//NE


//volScalarFieldValue ALLOTMENT

);


