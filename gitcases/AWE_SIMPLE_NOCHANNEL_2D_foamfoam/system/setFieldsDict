FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

//Start of Defining parametric variables //

H   30.0e-3;//cell HEIGHT in Y-direction
W	30.0e-3;//cell WIDTH in Z-direction


tp	1.5e-3; //thickness of positive electrode
tdi	0.5e-3; //thickness of diaphragm
tn	1.5e-3; //thickness of negative electrode

z0	0;
z1  30e-3;//cell depth in Z-direction


//Start of cumulative thickness calculation for each layer//

t1 #eval #{ $tp+$tdi #};//cumulative thickness of (positive electrode + di)
t2 #eval #{ $t1+$tn #};//cumulative thickness of  (positive electrode + di +negative electrode)

//End of cumulative thickness calculation for each layer//

/*
**********************************************************************************************************************
PE_setvalue_poredensity						110;
PE_setvalue_poresize						2.0000e-04;
PE_setvalue_tortuosity						1.20;
PE_setvalue_specificsurfacearea 			1.4141e+04;
PE_setvalue_viscosity						2.0000e-03;
PE_setvalue_porosity						0.5892;
PE_setvalue_permeability					1.7290e-10;
PE_setvalue_d								1.1568e+07;
PE_setvalue_porous_wire_dia					1.5455e-05;
PE_setvalue_porouscell_spacing				2.3091e-04;
PE_setvalue_esimatedsherwoodnumber			4.9101e-01;




DI_setvalue_poredensity						300;
DI_setvalue_poresize						5.0000e-05;
DI_setvalue_tortuosity						1.50;
DI_setvalue_specificsurfacearea 			3.2869e+04;
DI_setvalue_viscosity						2.0000e-03;
DI_setvalue_porosity						0.2739;
