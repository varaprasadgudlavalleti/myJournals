defaultFieldValues
(
    //volScalarFieldValue sigma_s 1000
    //volScalarFieldValue r_p 2e-05
    volScalarFieldValue Ne 0
    volScalarFieldValue Pe 0
    volScalarFieldValue Mem 0
    //volScalarFieldValue InRegion 0
    volScalarFieldValue eps 0.9999
    volScalarFieldValue	tau 1
);





regions
(


//eps ALLOTMENT

//epsallotment for PE region
    boxToCell
    {
    	box (0 0 $z0) ($tp $H $z1);

    	fieldValues
    	(
    		volScalarFieldValue eps $PE_setvalue_porosity
    	);
    }
//epsallotment for PE region

//epsallotment for DI region
    boxToCell
    {
    	box ($tp 0 $z0) ($t1 $H $z1);

    	fieldValues
    	(
    		volScalarFieldValue eps $DI_setvalue_porosity
    	);
    }
//epsallotment for DI region

//epsallotment for NE region
        boxToCell
    {
    	box ($t1 0 $z0) ($t2 $H $z1);

    	fieldValues
