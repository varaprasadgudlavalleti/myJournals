FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      eps;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //



//**********************************************************************************************************************//
PE_setvalue_poredensity						110;
PE_setvalue_poresize						2.0000e-04;
PE_setvalue_tortuosity						1.20;
PE_setvalue_specificsurfacearea 			1.4141e+04;
PE_setvalue_viscosity						2.0000e-03;
PE_setvalue_porosity						0.5892;
PE_setvalue_permeability					1.7290e-10;
PE_setvalue_d								1.1568e+07;
PE_setvalue_porous_wire_dia					1.5455e-05;
PE_setvalue_porouscell_spacing				2.3091e-04;
PE_setvalue_esimatedsherwoodnumber			4.9101e-01;




DI_setvalue_poredensity						300;
DI_setvalue_poresize						5.0000e-05;
DI_setvalue_tortuosity						1.50;
DI_setvalue_specificsurfacearea 			3.2869e+04;
DI_setvalue_viscosity						2.0000e-03;
DI_setvalue_porosity						0.2739;
DI_setvalue_permeability					3.9309e-13;
DI_setvalue_d								5.0879e+09;
DI_setvalue_porous_wire_dia					1.7333e-05;
DI_setvalue_porouscell_spacing				8.4667e-05;
DI_setvalue_esimatedsherwoodnumber			1.8261e-01;



NE_setvalue_poredensity						110;
NE_setvalue_poresize						2.0000e-04;
NE_setvalue_tortuosity						1.20;
NE_setvalue_specificsurfacearea 			1.4141e+04;
NE_setvalue_viscosity						2.0000e-03;
NE_setvalue_porosity						0.5892;
NE_setvalue_permeability					1.7290e-10;
NE_setvalue_d								1.1568e+07;
NE_setvalue_porous_wire_dia					1.5455e-05;
NE_setvalue_porouscell_spacing				2.3091e-04;
NE_setvalue_esimatedsherwoodnumber			4.9101e-01;
//**********************************************************************************************************************//


dimensions      [0 0 0 0 0 0 0];

//internalField   uniform 1.5;
internalField   uniform $PE_setvalue_porosity;


boundaryField
{

    PE
    {
        type            fixedValue;
        value           uniform $PE_setvalue_porosity;
    }
    DI
    {
        type            fixedValue;
        value           uniform $DI_setvalue_porosity;
    }
    NE
    {
        type            fixedValue;
        value           uniform $NE_setvalue_porosity;
    }

    INLET
    {
        type            fixedValue;
        value           uniform $NE_setvalue_porosity;
    }
    OUTLET
    {
        type            zeroGradient;
    }

    frontAndBack
    {
	type		empty;
    }

}


