FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

//internalField   uniform (0 0.518 0);//1400ml/min
internalField   uniform (0 0.74 0);//200ml/min


boundaryField
{
    PE
    {
        type            noSlip;
    }

    DI
    {
        type            noSlip;
    }
    NE
    {
        type            noSlip;
    }

    INLET
    {
    	//type	zeroGradient;
        type            fixedValue;
        value		$internalField;
        //type	flowRateInletVelocity;
        //volumetricFlowRate constant 0.833e-6;
        //type 			fixedMean;
        //meanValue		constant (VEX VEY VEZ);
        //value			$internalField;
    }

    OUTLET
    {
        type            zeroGradient;
    }


    frontAndBack
    {
	type		empty;
    }

}


